{
  "presets": [
    "@babel/preset-env",
    "@babel/preset-react",
    "@babel/preset-typescript"
  ],
  "plugins": [
    [
      "babel-plugin-module-resolver",
      {
        "root": ".",
        "alias": {
          "@": "./src"
        }
      }
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "antd",
        "libraryDirectory": "lib",
        "style": true
      },
      "antd"
    ],
    [
      "babel-plugin-import",
      {
        "libraryName": "@ant-design/icons",
        "libraryDirectory": "",
        "camel2DashComponentName": false
      },
      "@ant-design/icons"
    ]
  ]
}
