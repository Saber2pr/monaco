  "devDependencies": {
    "@types/nanoid": "^3.0.0",
    "@types/node": "^16.3.3",
    "@types/react": "^17.0.43",
    "@types/styled-components": "^5.1.25",
    "copyfiles": "^2.4.1",
    "husky": "^7.0.2",
    "lint-staged": "^11.1.2",
    "prettier": "^2.4.1",
    "standard-version": "^9.3.1",
